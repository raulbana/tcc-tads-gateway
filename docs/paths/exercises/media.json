{
  "put": {
    "summary": "Faz upload de mídias para um exercício.",
    "description": "Recebe um vetor de imagens e outro de vídeos para um exercício existente. O ID do exercício deve ser informado no path.",
    "tags": ["HU002 - Manter Exercícios"],
    "security": [
      { "bearerAuth": [] }
    ],
    "parameters": [
      {
        "name": "id",
        "in": "path",
        "required": true,
        "description": "ID do exercício criado",
        "schema": {
          "type": "string"
        }
      }
    ],
    "requestBody": {
      "required": true,
      "content": {
        "multipart/form-data": {
          "schema": {
            "type": "object",
            "properties": {
              "images": {
                "type": "array",
                "items": {
                  "type": "string",
                  "format": "binary"
                },
                "required": true,
                "description": "Arquivos de imagem para upload."
              },
              "videos": {
                "type": "array",
                "items": {
                  "type": "string",
                  "format": "binary"
                },
                "required": true,
                "description": "Arquivos de vídeo para upload."
              }
            }
          }
        }
      }
    },
    "responses": {
      "204": {
        "description": "Mídias adicionadas com sucesso."
      },
      "400": {
        "description": "Dados de requisição inválidos."
      },
      "401": {
        "description": "Token de autenticação ausente ou inválido."
      },
      "403": {
        "description": "Usuário não tem permissão para enviar mídias."
      },
      "404": {
        "description": "Exercício não encontrado."
      },
      "500": {
        "description": "Erro interno do servidor."
      }
    }
}
}